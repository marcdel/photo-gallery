<% provide(:title, "New Gallery") %>

<h1>New Gallery</h1>

<%= form_for(@gallery) do |f| %>
    <%= render 'shared/errors', object: f.object %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit "Create", class: "btn btn-large btn-primary" %>
<% end %>